{
  "name": "vite-pwa-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npm run lint:strict",
    "lint:relaxed": "ESLINT_RELAXED=true eslint .",
    "lint:strict": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy ./dist --project-name=vite-pwa-test",
    "deploy:preview": "npm run build && wrangler pages deploy ./dist --project-name=vite-pwa-test",
    "wrangler": "wrangler",
    "security": "docker run --rm -v \"${PWD}:/src\" returntocorp/semgrep semgrep --config=p/security-audit --config=p/secrets --config=p/owasp-top-ten --config=p/react --config=p/typescript --config=p/javascript --config=.semgrep.yml --json --output=semgrep-results.json"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-react-dom": "^2.3.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^2.3.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "wrangler": "^4.46.0"
  }
}
